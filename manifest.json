{
  "manifest_version": 2,

  "name": "chrome extention",
  "description": "proxy",
  "version": "1.0",
  "icons": {
    "128": "icon16.png",
    "48": "icon16.png",
    "16": "icon16.png"
  },

  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },

  "permissions": [
          "storage",
          "notifications",
          "contextMenus",
          "proxy"
        ]


}


